				$('#portfolio-filter li').removeClass('activeFilter');
				$(this).parent('li').addClass('activeFilter');
				var selector = $(this).attr('data-filter');
				$container.isotope({ filter: selector });
				return false;
			});

			$(window).resize(function() {
				$container.isotope('layout');
			});

		});

	jQuery(document).ready( function($){

		$(".range_23").ionRangeSlider({
			type: "double",
			min: 0.00,
			max: 400.00,
			from: 0.00,
			to: 400.00,
			postfix: 'DH',
			hide_min_max: true,
			hide_from_to: false,
			grid: false,
			onStart: function (data) {
				priceRangefrom = data.from;
				priceRangeto = data.to;
			},
			onFinish: function (data) {
				priceRangefrom = data.from;
				priceRangeto = data.to;

				$container.isotope({
					filter: function() {

						if( $(this).find('.product-price').find('ins').length > 0 ) {
							var price = $(this).find('.product-price ins').text();
						} else {
							var price = $(this).find('.product-price').text();
						}

						priceNum = price.split("DH");

						return ( priceRangefrom <= priceNum[0] && priceRangeto >= priceNum[0] );
					}
				});

			}
		});

	});
	</script>
